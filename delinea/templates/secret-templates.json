{
  "description": "Delinea Secret Server Template Definitions",
  "author": "Dominic M. Hoang",
  "note": "These templates match the mock data in delinea_client.py",
  "templates": [
    {
      "id": 1,
      "name": "Windows Account",
      "description": "Windows local or domain account credentials",
      "fields": [
        {
          "name": "Machine",
          "slug": "machine",
          "description": "Server hostname or IP address",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Username",
          "slug": "username",
          "description": "Account username",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Password",
          "slug": "password",
          "description": "Account password",
          "isPassword": true,
          "isRequired": true
        },
        {
          "name": "Notes",
          "slug": "notes",
          "description": "Additional notes",
          "isPassword": false,
          "isRequired": false,
          "isNotes": true
        }
      ],
      "vaultMapping": {
        "recommendedPath": "secret/windows/{machine}",
        "fieldMapping": {
          "Machine": "host",
          "Username": "username",
          "Password": "password",
          "Notes": "notes"
        }
      }
    },
    {
      "id": 2,
      "name": "Unix Account (SSH)",
      "description": "Linux/Unix SSH account credentials",
      "fields": [
        {
          "name": "Machine",
          "slug": "machine",
          "description": "Server hostname or IP address",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Username",
          "slug": "username",
          "description": "SSH username",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Password",
          "slug": "password",
          "description": "SSH password (if using password auth)",
          "isPassword": true,
          "isRequired": false
        },
        {
          "name": "Private Key",
          "slug": "private-key",
          "description": "SSH private key content",
          "isPassword": true,
          "isRequired": false
        },
        {
          "name": "Notes",
          "slug": "notes",
          "description": "Additional notes",
          "isPassword": false,
          "isRequired": false,
          "isNotes": true
        }
      ],
      "vaultMapping": {
        "recommendedPath": "secret/linux/{machine}",
        "fieldMapping": {
          "Machine": "host",
          "Username": "username",
          "Password": "password",
          "Private Key": "private_key",
          "Notes": "notes"
        }
      }
    },
    {
      "id": 3,
      "name": "Database (SQL Server)",
      "description": "SQL Server database credentials",
      "fields": [
        {
          "name": "Server",
          "slug": "server",
          "description": "Database server hostname",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Database",
          "slug": "database",
          "description": "Database name",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Username",
          "slug": "username",
          "description": "Database username",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Password",
          "slug": "password",
          "description": "Database password",
          "isPassword": true,
          "isRequired": true
        }
      ],
      "vaultMapping": {
        "recommendedPath": "secret/database/{server}-{database}",
        "fieldMapping": {
          "Server": "server",
          "Database": "database",
          "Username": "username",
          "Password": "password"
        }
      }
    },
    {
      "id": 4,
      "name": "Web Password",
      "description": "Web application credentials",
      "fields": [
        {
          "name": "URL",
          "slug": "url",
          "description": "Website URL",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Username",
          "slug": "username",
          "description": "Login username or email",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "Password",
          "slug": "password",
          "description": "Login password",
          "isPassword": true,
          "isRequired": true
        },
        {
          "name": "Notes",
          "slug": "notes",
          "description": "Additional notes",
          "isPassword": false,
          "isRequired": false,
          "isNotes": true
        }
      ],
      "vaultMapping": {
        "recommendedPath": "secret/web/{slug}",
        "fieldMapping": {
          "URL": "url",
          "Username": "username",
          "Password": "password",
          "Notes": "notes"
        }
      }
    },
    {
      "id": 5,
      "name": "API Key",
      "description": "API credentials for services",
      "fields": [
        {
          "name": "Service Name",
          "slug": "service-name",
          "description": "Name of the service or API",
          "isPassword": false,
          "isRequired": true
        },
        {
          "name": "API Key",
          "slug": "api-key",
          "description": "API key or client ID",
          "isPassword": true,
          "isRequired": true
        },
        {
          "name": "API Secret",
          "slug": "api-secret",
          "description": "API secret or client secret",
          "isPassword": true,
          "isRequired": false
        },
        {
          "name": "Endpoint URL",
          "slug": "endpoint-url",
          "description": "API endpoint URL",
          "isPassword": false,
          "isRequired": false
        }
      ],
      "vaultMapping": {
        "recommendedPath": "secret/api/{service-slug}",
        "fieldMapping": {
          "Service Name": "service_name",
          "API Key": "api_key",
          "API Secret": "api_secret",
          "Endpoint URL": "endpoint"
        }
      }
    }
  ]
}
